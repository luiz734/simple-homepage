{
    "manifest_version": 3,
    "name": "Mookbark",
    "version": "0.2.2",
    "description": "A simple homepage for browsers made with Svelte.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{4692de2c-c019-4678-b486-4b4e3132993d}",
            "data_collection_permissions": {
                "required": ["none"]
            }
        }
    },
    "action": {
        "default_title": "Mookbark"
    },
    "icons": {
        "48": "icons/border-48.svg"
    },
    "permissions": ["storage"]
}
